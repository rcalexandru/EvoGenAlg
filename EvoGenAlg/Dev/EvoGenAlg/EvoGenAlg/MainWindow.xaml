<Window x:Class="EvoGenAlg.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EvoGenAlg"
        mc:Ignorable="d"
        Title="MainWindow" Height="666" Width="666" Closed="Window_Closed">
    <Grid>
        <StackPanel Orientation="Vertical">
            <Button Name="btnStart" Click="btnStart_Click" Height="20" Content="Start mutations" Margin="5"/>
            <TextBlock Name="tbFitness" Margin="5" Height="20"/>
            <Canvas Name="rcDisplay" Width="500" Height="500" Margin="5"></Canvas>
        </StackPanel>
        
    </Grid>
</Window>
